<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="900.0" styleClass="bodybg" stylesheets="@../background.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="farmapp.controller.LayoutController">
   <right>
      <Pane prefHeight="496.0" prefWidth="103.0" BorderPane.alignment="CENTER">
         <children>
                <Button fx:id="profile" layoutX="16.0" layoutY="82.0" mnemonicParsing="false" onAction="#profileView" prefHeight="25.0" prefWidth="72.0" text="Profilo">
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font name="Cooper Black" size="12.0" />
               </font>
            </Button>
                <Button fx:id="logOut" layoutX="20.0" layoutY="489.0" mnemonicParsing="false" onAction="#exitAction" text="Log Out">
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font name="Cooper Black" size="12.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </right>
   <top>
      <HBox fx:id="menuBar" alignment="CENTER_LEFT" prefHeight="66.0" prefWidth="900.0" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
